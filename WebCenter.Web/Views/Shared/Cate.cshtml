    <script type="text/javascript" language="javascript">
        function display() {
            document.getElementById("sign").style.display = "block";
        }
        function disappear() {
            document.getElementById("sign").style.display = "none";
        }

        if ($(window).width() < 960) {
            $('.search').hide();
        }

        $('.search').click(function () {
            if ($('.searchimg').is(':visible')) {
                $('.searchimg').hide();
                $('.searchtext').show();
                $('.searchtext > input').val('');
                $('.searchtext > input').animate({ width: "150px" }, 500);
                $('.searchtext > input').focus();
            }
        });

        var event = event || window.event

        $('.searchtext > input').keyup(function () {
            if (event.keyCode == 13) {
                $('.searchForm').submit();
            }
        });


        //search page  无效
        $('.searchtext > input').blur(function () {
            $('.searchtext > input').animate({ width: "0px" }, 500);
            setTimeout(function () {
                $('.searchtext').hide();
                $('.searchimg').show();
            }, 500);

        });

        //未登录
        $('.pregister').click(function () {
            $('.pregister').attr('href', 'javascript:void(0)');
            $('#sign').show();
        });


    </script>
